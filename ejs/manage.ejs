    <script>
        var clearMessage = function(userName){
            $.get('clearMessage',{
                "userName":userName
            },function(res){
                if(res == 1)
                {
                    showtext("操作成功！")
                    setTimeout(function(){
                        window.location.href = window.location.href
                    },1000)
                }
                else
                {
                    showtext("操作失败")
                }
            })
        }
        $("#time").html(new Date().toLocaleTimeString())
        setInterval(() => {
            $("#time").html(new Date().toLocaleTimeString())
